[Unit]
Description=Kubernetes Kube-Proxy Server
Documentation=https://github.com/GoogleCloudPlatform/kubernetes
After=network.target

[Service]
EnvironmentFile={{ k8s_cfg_dir }}/kube-proxy.conf
ExecStart={{ k8s_bin_dir }}/kube-proxy $KUBE_PROXY_OPTS
Restart=always
RestartSec=5
LimitNOFILE=65536

[Install]
WantedBy=multi-user.target
